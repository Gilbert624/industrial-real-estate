# Streamlit Secrets 配置示例
# 复制此文件为 secrets.toml 并填入实际值（不要提交 secrets.toml 到 Git）

# Anthropic API Key (用于 AI Assistant)
ANTHROPIC_API_KEY = "sk-ant-your-api-key-here"

# 数据库配置
[database]
type = "sqlite"
path = "industrial_real_estate.db"

# 应用配置
[app]
env = "production"
debug = false

# 可选：其他 API Keys
# [api_keys]
# world_bank_api = "your-key"
# abs_api = "your-key"
